# aliases
alias g=git
alias q=exit
alias r=reset
alias v=vim
# git aliases
alias gs='git status'
alias gaa='git add --all'
alias gcm='git commit -m'
alias gcm.='git commit -m "."'
alias gpom='git push origin main'
alias gpo='git push origin'
alias gd='git diff'
alias gp='git pull'
# python aliases
alias p=python
alias i=ipython

# sources
source $HOME/dotfiles/nnnrc
